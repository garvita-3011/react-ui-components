{
  "presets": [
    ["@babel/preset-env", {"modules": false}],
    "@babel/preset-react"
  ],
  "plugins": [
    // ["no-side-effect-class-properties"],
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ],
    ["transform-react-remove-prop-types", {
      "mode": "unsafe-wrap"
    }]
  ]
}